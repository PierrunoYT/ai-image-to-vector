# Recraft API Token
# Replace with your actual API token from Recraft
# Required for vectorization
RECRAFT_API_TOKEN=your_token_here

# Replicate API Token
# Replace with your actual API token from Replicate
# Used for Ideogram image generation (optional if using OpenAI or Fal.ai)
REPLICATE_API_TOKEN=your_replicate_token_here

# Fal.ai API Key
# Replace with your actual API key from Fal.ai
# Used for Ideogram image generation (optional if using OpenAI or Replicate)
FAL_KEY=your_fal_api_key_here

# OpenAI API Key
# Replace with your actual API key from OpenAI
# Used for GPT-Image-1 or DALL-E 3 image generation (optional if using Replicate or Fal.ai)
OPENAI_API_KEY=your_openai_api_key_here
